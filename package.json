  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "bcryptjs": "^3.0.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
