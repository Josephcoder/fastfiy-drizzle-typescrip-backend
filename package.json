{
  "name": "fastfiy-drizzle-typescrip-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "db:generate": "npx drizzle-kit generate",
    "migrate:dev": "cross-env DB_MIGRATING=true tsx src/scripts/migrate-db.ts",
    "migrate:prod": "cross-env DATABASE_URL=$PROD_DATABASE_URL DB_MIGRATING=true ts-node src/scripts/migrate-db.ts",
    "db:seed": "cross-env DB_SEEDING=true tsx src/scripts/seed-data.ts"
  },
