    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/pdfkit": "^0.17.2",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "ts-node-dev": "^2.0.0",
